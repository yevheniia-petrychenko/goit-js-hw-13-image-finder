{{#each this}}
<li class="gallery-item">
    <div class="photo-card">
        <img class="photo-card-img" src="{{webformatURL}}" data-src="{{largeImageURL}}" alt="{{tags}}" />
        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up</i><span class="value">{{likes}}</span>
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i><span class="value">{{views}}</span>
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i><span class="value">{{comments}}</span>
            </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i><span class="value">{{downloads}}</span>
            </p>
        </div>
    </div>
</li>
{{/each}}